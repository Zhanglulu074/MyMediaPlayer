<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTitleBar">
        <attr name="title_bar_left_src" format="reference" />
        <attr name="title_bar_right_src" format="reference" />
        <attr name="title_bar_title" format="string" />
    </declare-styleable>

    <declare-styleable name="MusicProgressButton">
        <attr name="progress" format="float"/>
        <attr name="max" format="float"/>
    </declare-styleable>

    <declare-styleable name="RoundRectImageView">
        <attr name="image_radius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MusicPlayBackGround">
        <attr name="circleColor" format="color"/>
        <attr name="minRadius" format="dimension"/>
        <attr name="maxRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="music_image_radius" format="dimension"/>
    </declare-styleable>

</resources>